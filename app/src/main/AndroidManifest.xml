<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="login.learn2develop.net.login" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".BasicViews6Activity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="Name Activity"
            android:name=".NameActivity" >
            <intent-filter >
                <action android:name="net.learn2develop.NameActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:label="Number Activity"
            android:name=".NumberActivity" >
            <intent-filter >
                <action android:name="net.learn2develop.NumberActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:label="PassWord Activity"
            android:name=".PasswordActivity" >
            <intent-filter >
                <action android:name="net.learn2develop.PasswordActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:label="Login Activity"
            android:name=".LoginActivity" >
            <intent-filter >
                <action android:name="net.learn2develop.LoginActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:label="Change Activity"
            android:name=".ChangeActivity" >
            <intent-filter >
                <action android:name="net.learn2develop.ChangeActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
